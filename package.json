{
	"name": "@doclify/javascript",
	"version": "3.0.2",
	"description": "Official JavaScript SDK for Doclify",
	"main": "dist/doclify-javascript.common.js",
	"module": "dist/doclify-javascript.esm.js",
	"unpkg": "dist/doclify-javascript.js",
	"jsdelivr": "dist/doclify-javascript.js",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/doclify/doclify-javascript.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src/*",
		"dist/*"
	],
	"keywords": [
		"doclify",
		"javascript",
		"sdk"
	],
	"scripts": {
		"dev": "node examples/server.js",
		"dev:dist": "rollup -wm -c build/rollup.dev.config.js",
		"build": "node build/build.js",
		"lint": "eslint src test",
		"test": "npm run lint && npm run test:unit",
		"flow": "flow check",
		"test:unit": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
		"release": "bash scripts/release.sh"
	},
	"homepage": "https://github.com/doclify/doclify-javascript#readme",
	"author": "Andrej Adamcik",
	"license": "MIT",
	"peerDependencies": {},
	"devDependencies": {
		"babel-core": "^6.24.1",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^7.1.3",
		"babel-preset-env": "^1.6.1",
		"buble": "^0.19.8",
		"cross-spawn": "^6.0.5",
		"css-loader": "^2.1.1",
		"dotenv": "^8.0.0",
		"es6-promise": "^4.2.8",
		"eslint": "^4.19.1",
		"eslint-plugin-jasmine": "^2.10.1",
		"express": "^4.17.1",
		"express-urlrewrite": "^1.2.0",
		"jasmine": "2.8.0",
		"lint-staged": "^8.2.0",
		"path-to-regexp": "^1.7.0",
		"rollup": "^1.20.1",
		"rollup-plugin-auto-external": "^2.0.0",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-buble": "^0.19.8",
		"rollup-plugin-commonjs": "^10.0.2",
		"rollup-plugin-flow-no-whitespace": "^1.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-watch": "^4.0.0",
		"terser": "^4.2.0",
		"webpack": "^4.35.2",
		"webpack-dev-middleware": "^3.7.0"
	},
	"dependencies": {
		"axios": ">=0.19.2"
	}
}
