{
  "name": "@doclify/javascript",
  "version": "4.0.0-beta.0",
  "description": "ðŸ“¦ Javascript SDK for Doclify API",
  "keywords": [
    "doclify",
    "doclify-javascript",
    "doclify-sdk",
    "doclify-sdk-js",
    "doclify-sdk-javascript",
    "sdk",
    "api",
    "http",
    "rest",
    "restful",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/doclify/doclify-javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:doclify/doclify-javascript.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "siroc build --watch",
    "build": "siroc build",
    "clean": "rm -rf bin dist coverage",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "test": "run-s build lint test:unit",
    "test:unit": "jest",
    "commit": "git-cz",
    "release": "dotenv release-it --",
    "prepare": "siroc build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/js-cookie": "^3.0.0",
    "@types/qs": "^6.9.6",
    "axios": "^0.24.0",
    "defu": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@release-it/conventional-changelog": "4.1.0",
    "@types/jest": "27.4.0",
    "@types/sinon": "10.0.6",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "dotenv-cli": "4.1.1",
    "eslint": "8.6.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "npm-run-all": "4.1.5",
    "release-it": "14.12.3",
    "sinon": "12.0.1",
    "siroc": "0.16.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
